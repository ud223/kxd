            <div class="container-fluid">
                <!-- BEGIN LOGO -->
                <a class="brand" href="/">
                    <img src="~/media/image/logo.png" alt="logo" />
                </a>
                <!-- END LOGO -->
                <!-- BEGIN RESPONSIVE MENU TOGGLER -->
